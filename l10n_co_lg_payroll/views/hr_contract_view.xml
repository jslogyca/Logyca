<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
	<record id="hr_contract_view_form_integral" model="ir.ui.view">
		<field name="name">hr.contract.view.form.integral</field>
		<field name="model">hr.contract</field>
		<field name="inherit_id" ref="hr_contract.hr_contract_view_form"/>
		<field name="arch" type="xml">
			<xpath expr="//field[@name='job_id']" position="after">
                <field name="contract_integral"/>
                <field name="pensionado"/>
                <field name="risk_id" required="1"/>
                <field name="aux_transp_full" />
			</xpath>
		</field>
	</record>
</odoo>