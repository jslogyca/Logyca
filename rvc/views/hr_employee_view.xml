<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_employee_form_inherit_crecemype" model="ir.ui.view">
        <field name="name">hr.employee.form.inherit.crecemype</field>
        <field name="model">hr.employee</field>
        <field name="inherit_id" ref="hr.view_employee_form"/>
        <field name="arch" type="xml">
            <xpath expr="//sheet/notebook/page[@name='public']" position="inside">
                <group string="LOGYCA / CRECEMYPE">
                    <field name="x_is_crecemype_consultant"/>
                </group>
            </xpath>
        </field>
    </record>
</odoo>
