<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="account_sector_macro_view_tree_rvc" model="ir.ui.view">
        <field name="name">account.sector.macro.tree.rvc</field>
        <field name="model">account.sector.macro</field>
        <field name="arch" type="xml">
            <tree string="Sector/MacroSector" >
                <field name="macro_sector" />
                <field name="sector_id" />
            </tree>
        </field>
    </record>	

    <record id="action_account_sector_macro_rvc" model="ir.actions.act_window">
        <field name="name">Sector/MacroSector</field>
        <field name="res_model">account.sector.macro</field>
        <field name="view_mode">tree</field>
        <field name="view_id" ref="account_sector_macro_view_tree_rvc"/>
    </record>

    <menuitem
        id="hr_menu_account_sector_macro_rvc"
        name="Sectores/Macrosectores"
        action="action_account_sector_macro_rvc"
        parent="rvc.menu_config_rvc_parent"
        sequence="10"/>
</odoo>
