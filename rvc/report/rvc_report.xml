            <field name="report_name">rvc.report_rvc_bienv</field>
            <field name="report_file">rvc.report_rvc_bienv</field>
            <field name="print_report_name">'rvc - %s' % (object.partner_id.partner_id.name)</field>
            <field name="binding_model_id" ref="model_product_benef"/>
            <field name="binding_type">report</field>
        </record>
</odoo>