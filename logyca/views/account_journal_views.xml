<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="logyca_account_journal_form" model="ir.ui.view">
            <field name="name">logyca.account.journal.form</field>
            <field name="model">account.journal</field>
            <field name="inherit_id" ref="account.view_account_journal_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='refund_sequence_number_next']" position="after">
                    <field name="x_resolution_number"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
