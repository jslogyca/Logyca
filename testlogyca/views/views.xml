<!-- //////////////////////////////// Test VIEWS //////////////////////////////// -->    
<odoo>
  <data>
    <!-- Lideres VIEWS -->    
    <record id="testlogyca_menu_action" model="ir.actions.act_window">
        <field name="name">Lideres</field>
          <field name="res_model">testlogyca.testlogyca</field>
        <field name="view_mode">tree,form,kanban</field>
        <field name="domain">[]</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">Crear formulario de ejemplo
            </p>
        </field>
    </record>

    <record model="ir.ui.view" id="testlogyca.list">
      <field name="name">testlogyca list</field>
      <field name="model">testlogyca.testlogyca</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="age"/>
          <field name="date"/>
          <field name="gender"/>
          <field name="nationality"/>
        </tree>
      </field>
    </record>

    <!-- Empleados VIEWS -->    
    <record id="testlogyca_menu_action_parent" model="ir.actions.act_window">
        <field name="name">Empleados</field>
          <field name="res_model">testlogyca.persontocargo</field>
        <field name="view_mode">tree,form,kanban</field>
        <field name="domain">[]</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">Crear formulario de ejemplo
            </p>
        </field>
    </record>

    <record model="ir.ui.view" id="testlogyca.listparent">
      <field name="name">testlogyca list 2</field>
      <field name="model">testlogyca.persontocargo</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="age"/>
          <field name="date"/>
          <field name="gender"/>          
        </tree>
      </field>
    </record>
  </data>
</odoo>